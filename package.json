{
  "name": "onlineshop_copy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build",
    "dev:client": "webpack serve --env dev",
    "build:client:dev": "webpack --env dev",
    "build:client": "webpack",
    "build:server": "tsc -p tsconfig.server.json",
    "build:dev": "npm run build:client:dev && npm run build:server",
    "build": "npm run build:client && npm run build:server",
    "start": "node ./build/server/app.js",
    "start:background": "nohup node ./build/server/app.js &",
    "deploy": "aws s3 sync ./build/client s3://onlineshop.com --profile=yongseong",
    "invalidate": "aws cloudfront create-invalidation --profile=yongseong --distribution-id EAMPBCES4Y7T1 --paths \"/*\"",
    "lint:client": "eslint \"./src/client/**/*.{ts,tsx}\"",
    "lint:server": "eslint ./src/server/**/*.ts",
    "test:client": "jest ./src/test/client",
    "test:server": "jest ./src/test/server"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub",
      "\\.(css|scss)$": "jest-transform-stub"
    },
    "testEnvironment": "jest-environment-jsdom"
  },
